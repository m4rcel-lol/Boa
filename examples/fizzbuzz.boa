imp io

# FizzBuzz - a classic programming exercise
fn fizzbuzz(n):
    for i in range(1, n + 1):
        if i % 15 == 0:
            io.print("FizzBuzz")
        elif i % 3 == 0:
            io.print("Fizz")
        elif i % 5 == 0:
            io.print("Buzz")
        else:
            io.print(i)

io.print("FizzBuzz 1-20:")
fizzbuzz(20)
